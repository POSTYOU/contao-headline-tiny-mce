{
  "name": "postyou/headline-tiny-mce",
  "description": "HTML headline for Contao",
  "keywords": [
    "contao",
    "headline",
    "html",
    "tiny-mce"
  ],
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Gerald Meier",
      "role": "Developer"
    },
    {
      "name":"postyou",
      "homepage":"http://postyou.de"
    }
  ],
  "require": {
    "php": "^7.0",
    "contao/core-bundle": "^4.4.0"
  },
  "autoload": {
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/cache/",
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ],
    "psr-4": {
      "Postyou\\HeadlineTinyMceBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Postyou\\HeadlineTinyMceBundle\\ContaoManager\\Plugin"
  }

}